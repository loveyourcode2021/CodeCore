<% @posts.each do |post| %>
  <div>
    <%= post.title %>
    <%= post.body%>
    <small>Posted:<%= time_ago_in_words(answer.created_at) %> ago</small>
      <%= button_to "Delete", question_answer_path(@question, answer), method: :delete,form: { data: { turbo_confirm: "Are you sure?" } } %>
    <hr>
  </div>
<% end %>